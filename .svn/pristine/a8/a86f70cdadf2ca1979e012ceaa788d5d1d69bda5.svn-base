public class LimitDTO {
    /**
     * 限额ID
     */
    private String id;

    /**
     * 限额名称
     */
    private String name;

	 /**
	  * 描述
	  */
	private String description;
	/**
	 * 费率集合
	 */
	private List<FloatRate> floatRateList;
	 
    /**
     * 限额值列表
     */
    private List<ValueDTO> valueList;
    
	public List<FloatRate> getFloatRateList() {
		return floatRateList;
	}

	public void setFloatRateList(List<FloatRate> floatRateList) {
		this.floatRateList = floatRateList;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<ValueDTO> getValueList() {
		return valueList;
	}

	public void setValueList(List<ValueDTO> valueList) {
		this.valueList = valueList;
	}

	 public String getDescription() {
		 return description;
	 }

	 public void setDescription(String description) {
		 this.description = description;
	 }
 }
