/*
id: 责任ID,
name: 责任名称,
riskInfoId: 险种ID,
limitList: [{
*/
public class LiabilityDTO {
    /**
     * 责任ID
     */
    private String id;

    /**
     * 责任名称
     */
    private String name;

    /**
     * 险种ID
     */
    private String riskInfoId;

    /**
     * 限额列表
     */
    private List<LimitDTO> limitList;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getRiskInfoId() {
		return riskInfoId;
	}

	public void setRiskInfoId(String riskInfoId) {
		this.riskInfoId = riskInfoId;
	}

	public List<LimitDTO> getLimitList() {
		return limitList;
	}

	public void setLimitList(List<LimitDTO> limitList) {
		this.limitList = limitList;
	}
}
