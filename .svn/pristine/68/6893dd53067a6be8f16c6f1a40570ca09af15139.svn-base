server:
    port: 6021
    undertow:
        accessLog:
            enabled: true
            pattern: "%t %a \"%r\" %s (%D ms)"
            dir: /jtpf/logs/${spring.application.name}
spring:
#    profiles:
#        active: @build.profile.id@
    application:
        name: prod-svc
#    zipkin:
#        base-url: http://localhost:9761
    datasource:
        type: com.zaxxer.hikari.HikariDataSource
        url: jdbc:mysql://10.30.131.42/jtproducts?characterEncoding=utf-8
        hikari:
            username: jtproducts
            password: "@2018#"
            driver-class-name: com.mysql.jdbc.Driver
            minimum-idle: 1
            maximum-pool-size: 5
            pool-name: dbcp1
            connection-test-query: select 1
management:
    security:
        enabled: false
        
eureka:
    client:
        registerWithEureka: true
        fetchRegistry: true
        serviceUrl:
            defaultZone: http://localhost:8761/eureka
    instance:
        prefer-ip-address: true
        hostname: ${spring.cloud.client.ipAddress}
        instance-id: ${spring.cloud.client.ipAddress}:${server.port}
        
mybatis:
  type-aliases-package: com.jt.plt.product.entity
  mapper-locations: classpath:mapper/*.xml
  
mapper:
  mappers: com.jt.plt.product.base.MyMapper
  not-empty: false
  identity: MYSQL

pagehelper:
  helperDialect: mysql
  reasonable: true
  supportMethodsArguments: true
  params: count=countSql
        