package com.jt.plt.product.dto.program;

import java.util.List;

public class ProgramInfoBean {
	/**
	 * 方案编码
	 */
	private String code;
	/**
	 * 固定-1/浮动-3/固定加浮动-4
	 */
	private String fixFloatFlag; 
	/**
	 *  主险-M/附加险标记-A
	 */
	private String riskFlag;
	/**
	 * 固定保费
	 */
	private Double premium;
	/**
	 * 险种集合
	 */
	private List<RiskInfoBean> riskList;
	
	/**
	 * @return the premium
	 * 固定保费
	 */
	public Double getPremium() {
		return premium;
	}
	/**
	 * @param 固定保费
	 */
	public void setPremium(Double premium) {
		this.premium = premium;
	}
	/**
	 * @return the code
	 *  方案编码
	 */
	public String getCode() {
		return code;
	}
	/**
	 * @param  方案编码
	 */
	public void setCode(String code) {
		this.code = code;
	}
	/**
	 * @return the fixFloatFlag
	 * 固定-1/浮动-3/固定加浮动-4
	 */
	public String getFixFloatFlag() {
		return fixFloatFlag;
	}
	/**
	 * @param 固定-1/浮动-3/固定加浮动-4
	 */
	public void setFixFloatFlag(String fixFloatFlag) {
		this.fixFloatFlag = fixFloatFlag;
	}
	/**
	 * @return the riskFlag
	 *   主险-M/附加险标记-A
	 */
	public String getRiskFlag() {
		return riskFlag;
	}
	/**
	 * @param   主险-M/附加险标记-A
	 */
	public void setRiskFlag(String riskFlag) {
		this.riskFlag = riskFlag;
	}
	/**
	 * @return the riskList
	 * 险种集合
	 */
	public List<RiskInfoBean> getRiskList() {
		return riskList;
	}
	/**
	 * @param 险种集合
	 */
	public void setRiskList(List<RiskInfoBean> riskList) {
		this.riskList = riskList;
	}
	
}
